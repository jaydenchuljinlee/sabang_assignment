	<!-- 실행에 필요한 자바스크립트 파일들 -->
	<!--                                   -->



	<script>
		$(document).ready(function() {

			/* 
			   기본정보로 페이지 이동
			*/
			$("#join_default").on("click",function() {
				
				var isChecked = $("#all-agree").attr("class");

				if (isChecked.indexOf("agree-checked") == -1) {

					alert("동의 항목을 체크해 주세요!");
					return;
				}

				var page = $(this).attr("id");

				$("#page_move").attr("action",page+".html");
				$("#page_move").submit();
			})

			/* 
			   서브 동의 항목 클릭 함수
			   @chk		: 동의를 클릭한 div태그
			   @this	: 이벤트리스너 안에서 this는 클릭한 객체를 가리킨다
			*/
			$(".child-agree").on("click",function() {
				var chk = $(this).attr("class");

				//체크를 명시해주는 클래스를 갖고 있을 때는 true문을 탄다.
				if (chk.indexOf("child-agree-checked") != -1) {

					//이벤트리스너의 객체(여기서 동의를 클릭한 div)의 체크를 풀어주고,상위 객체의 동의 div 또한 제거해준다.
					$(this).removeClass("child-agree-checked");
					$("#all-agree").removeClass("agree-checked");

					$(this).children().eq(1).show();
				
				} else {
					$(this).addClass("child-agree-checked");
					$(this).children().eq(1).hide();

					//서브 동의 div의 체크표시 개수
					var len = $(".child-agree-checked").length;

					//서브 동의가 두개 다 체크 돼있으면, 전체 동의를 하이라이트
					if (len == 2) {
						
						$("#all-agree").addClass("agree-checked");
					}
				}
			});

			/* 
			   전체 동의 항목 클릭 함수
			   @chk			: 동의를 클릭한 div태그
			   @nodes		: 서브 동의 항목들
			   @span_1,2	: 서브 동의 항목의 (필수) span태그
			   @this		: 이벤트리스너 안에서 this는 클릭한 객체를 가리킨다
			*/
			$("#all-agree").on("click",function() {
				var chk		= $(this).attr("class"),
				    nodes	= $(".child-agree").children(),
					span_1	= nodes[1],
					span_2	= nodes[6];

				//체크를 명시해주는 클래스를 갖고 있을 때는 true문을 탄다.
				if (chk.indexOf("agree-checked") != -1) {
					
					//이벤트리스너의 객체(여기서 동의를 클릭한 div)의 체크를 풀어주고,하위 객체들의 동의 div 또한 제거해준다.
					$(this).removeClass("agree-checked");
					$(".child-agree").removeClass("child-agree-checked");
						
					$(span_1).show();
					$(span_2).show();

				} else {
					
					$(this).addClass("agree-checked");
					$(".child-agree").addClass("child-agree-checked");
						
					$(span_1).hide();
					$(span_2).hide();
				}
			});
		});
	</script>
